{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {# the web debug toolbar content #}
    {% set icon %}
      <img src="data:image/png;base64," alt="Aura" />
    <span class="sf-toolbar-value">{{ collector.count }} in
        {% if collector.duration < 0.01 %}
        {{ '%0.2f'|format(collector.duration * 1000) }} ms
        {% else%}
        {{ '%.0f'|format(collector.duration * 1000) }} ms
        {% endif %}
    </span>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>DB Queries</b>
        <span>{{ collector.count }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Query time</b>
        <span>{{ '%0.2f'|format(collector.duration * 1000) }} ms</span>
    </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    {# the menu content #}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="Aura" /></span>
        <strong>AuraSql</strong>
        <span class="count">
            <span>{{ collector.count }}</span>
            <span>{{ '%0.0f'|format(collector.duration * 1000) }} ms</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    {# the panel content #}
    <table>
        <thead>
            <tr>
                <th>Duration</th>
                <th>Function</th>
                <th style="min-width: 600px">Statement</th>
                <th style="min-width: 30%">Bind parameters</th>
            </tr>
        </thead>
        <tbody>
        {% for profile in collector.profiles %}
            <tr>
                <td>
                    {{ profile.duration|number_format(4) }}
                </td>
                <td>
                    {{ profile.function }}
                </td>
                <td>
                    <pre>{{ profile.statement }}</pre>
                </td>
                <td style="max-width: 200px">
                    {{ profile.bind_values|json_encode(128) }}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
